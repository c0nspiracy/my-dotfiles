# vim: ft=zsh

# cd
alias ..='cd ..'

# ls
alias ls='ls -F --color'
alias l='ls -lAhG --color'
alias ll='ls -lG --color'
alias la='ls -AF --color'

# git
alias gl='git pull'
alias gp='git push'
alias gd='git diff'
alias gc='git commit'
alias gca='git commit -a'
alias gco='git checkout'
alias gb='git branch'
alias gs='git status'

