# vim: ft=zsh

# cd
alias ..='cd ..'

# ls
alias ls='ls -F'
alias l='ls -lAhG'
alias ll='ls -lG'
alias la='ls -AF'

# git
alias gl='git pull'
alias gp='git push'
alias gd='git diff'
alias gc='git commit'
alias gca='git commit -a'
alias gco='git checkout'
alias gb='git branch'
alias gs='git status'

